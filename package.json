{
	"name": "atom-mocha",
	"version": "v2.2.0",
	"description": "Run Atom specs using bleeding-edge Mocha and Chai.",
	"repository": "https://github.com/Alhadis/Atom-Mocha",
	"author": "John Gardner <gardnerjohng@gmail.com>",
	"license": "ISC",
	"main": "./index.js",
	"atomTestRunner": "./index.js",
	"scripts": {
		"lint": "npx eslint .",
		"test": "atom -t ./spec",
		"install": "node ./bin/post-install.js"
	},
	"engines": {
		"atom": ">1.6.0"
	},
	"files": ["bin", "lib", "index.js"],
	"dependencies": {
		"chai": "^4.2.0",
		"diff": "^4.0.1",
		"mocha": "^6.1.4",
		"mocha-when": "^1.0.0",
		"print": "^1.1.0"
	},
	"devDependencies": {
		"@alhadis/eslint-config": "^1.2.0",
		"atom-ts-transpiler": "^1.5.2",
		"eslint": "^5.16.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"typescript": "^2.0.0"
	},
	"atomTranspilers": [{
		"transpiler": "atom-ts-transpiler",
		"glob": "spec/**/*.{ts,tsx}",
		"options": {
			"compilerOptions": {"jsx": "react"},
			"cacheKeyFiles": [],
			"verbose": false
		}
	}]
}
