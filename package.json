{
	"name": "atom-mocha",
	"version": "v2.2.1",
	"description": "Run Atom specs using Mocha and Chai.",
	"repository": "https://github.com/Alhadis/Atom-Mocha",
	"author": "John Gardner <gardnerjohng@gmail.com>",
	"license": "ISC",
	"main": "./index.js",
	"atomTestRunner": "./index.js",
	"scripts": {
		"lint": "npx eslint .",
		"test": "atom -t ./spec",
		"install": "node ./bin/post-install.js"
	},
	"engines": {
		"atom": ">=1.35.0"
	},
	"files": ["bin", "lib", "index.js"],
	"dependencies": {
		"chai": "^4.3.4",
		"chinotto": "^1.0.0",
		"diff": "^5.0.0",
		"mocha": "^9.0.2",
		"mocha-when": "^1.0.1",
		"print": "^1.2.0"
	},
	"devDependencies": {
		"@alhadis/eslint-config": "^2.3.2",
		"atom-ts-transpiler": "^1.5.2",
		"eslint": "^7.30.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"typescript": "^3.8.3"
	},
	"atomTranspilers": [{
		"transpiler": "atom-ts-transpiler",
		"glob": "spec/**/*.{ts,tsx}",
		"options": {
			"compilerOptions": {"jsx": "react"},
			"cacheKeyFiles": [],
			"verbose": false
		}
	}]
}
